        {
            "type": "node",
            "request": "launch",
            "name": "Launch",
            "program": "${workspaceRoot}\\app.js",
            "cwd": "${workspaceRoot}",
            "env": {
				"NODE_ENV": "development"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Debug",
